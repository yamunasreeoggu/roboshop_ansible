[Unit]
Description = Catalogue Service

[Service]
User=roboshop
Environment=DOCUMENTDB=true
Environment=MONGO_URL="{{ lookup('aws_ssm', '{{env}}.roboshop.catalogue.MONGO_URL', region='us-east-1' ) }}"
ExecStart=/bin/node /app/server.js
SyslogIdentifier=catalogue

[Install]
WantedBy=multi-user.target

'mongodb://username:password@mongodb:27017/catalogue?tls=true&replicaSet=rs0&readPreference=secondaryPreferred&retryWrites=false'